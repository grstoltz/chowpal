{
  "name": "chowpal",
  "version": "1.0.0",
  "description": "ChowPal",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "client": "cd client && npm run start",
    "start": "concurrently \"./node_modules/.bin/nodemon\" \"npm run client\"",
    "build": "cd client && npm run build",
    "deploy": "yarn build && git add . && git commit -m \"Building for production\" && git push heroku master",
    "test": "env NODE_ENV=test mocha --exit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^5.0.5",
    "mocha-prepare": "^0.1.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "aws-sdk": "^2.221.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.15.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.6",
    "zebra-crossing": "^0.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grstoltz/chowpal.git"
  },
  "bugs": {
    "url": "https://github.com/grstoltz/chowpal/issues"
  },
  "homepage": "https://github.com/grstoltz/chowpal#readme"
}
